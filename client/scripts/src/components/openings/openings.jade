.row
   .col-md-9.col-md-offset-3
      a.btn.btn-primary(href="/app/openings/new") Create
hr
.row
   .col-md-3
      ul.list-unstyled(data-bind="foreach: openings")
         li: a(href="/app/openings/:id", data-bind="text: title, click: $parent.select")
   .col-md-9(data-bind="if: selected")
      h3(data-bind="text: selected().title()")
      p(data-bind="text: selected().description()")
